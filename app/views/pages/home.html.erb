        /* Le menu lui-m√™me - mode sombre */
        .mobile-menu-new.dark {
          position: fixed;
          top: 0;
          left: 0;
          width: 100%;
          height: 100vh;
          background-color: #121212; /* Fond noir pour mode sombre */

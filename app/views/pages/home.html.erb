<%# /* JS pour menu dÃ©roulant mobile */ %>
    <script>
      document.addEventListener('DOMContentLoaded', function() {
        // SÃ©lectionner tous les liens du menu
        var menuLinks = document.querySelectorAll('.mobile-menu-list a');
        var menuToggle = document.getElementById('menu-toggle');

        // Ajouter un Ã©couteur d'Ã©vÃ©nement Ã  chaque lien
        menuLinks.forEach(function(link) {
          link.addEventListener('click', function(e) {
            // EmpÃªcher le comportement par dÃ©faut du lien pour contrÃ´ler la navigation
            e.preventDefault();

            // RÃ©cupÃ©rer l'ID de la section cible
            var targetId = link.getAttribute('href');

            // Fermer le menu en dÃ©cochant la case Ã  cocher
            menuToggle.checked = false;

            // Attendre que l'animation de fermeture soit terminÃ©e avant de faire dÃ©filer
            setTimeout(function() {
              // Trouver l'Ã©lÃ©ment correspondant
              var targetElement = document.querySelector(targetId);

              // Faire dÃ©filer jusqu'Ã  la section cible
              if (targetElement) {
                // Calculer la position de la section cible
                var offset = targetElement.getBoundingClientRect().top + window.pageYOffset;

                // Faire dÃ©filer la page vers la section cible
                window.scrollTo({
                  top: offset,
                  behavior: 'smooth'
                });
              }
            }, 300); // DÃ©lai correspondant Ã  l'animation du menu
          });
        });
      });
    </script>

    <%# /* JS pour liens footer : renvoient bien en haut de chaque section */ %>
    <script>
          document.addEventListener('DOMContentLoaded', function() {
      // Attendre que tout soit complÃ¨tement chargÃ©
      setTimeout(function() {

        function scrollToSectionTop(targetId) {
          var targetElement = document.querySelector(targetId);
          if (targetElement) {
            // MÃ©thode alternative : utiliser offsetTop
            var elementTop = targetElement.offsetTop;

            // Animation plus lente et plus fluide
            var startPosition = window.pageYOffset;
            var distance = elementTop - startPosition;
            var duration = Math.max(800, Math.abs(distance) * 0.5); // DurÃ©e minimum 800ms
            var startTime = null;

            function animateScroll(currentTime) {
              if (startTime === null) startTime = currentTime;
              var timeElapsed = currentTime - startTime;
              var progress = Math.min(timeElapsed / duration, 1);

              // Fonction d'easing pour une animation plus douce
              var easeInOutQuad = progress < 0.5
                ? 2 * progress * progress
                : 1 - Math.pow(-2 * progress + 2, 2) / 2;

              var currentPosition = startPosition + (distance * easeInOutQuad);
              window.scrollTo(0, currentPosition);

              if (progress < 1) {
                requestAnimationFrame(animateScroll);
              }
            }

            requestAnimationFrame(animateScroll);
          }
        }

        // DÃ©sactiver le comportement Webflow existant en premier
        var webflowLinks = document.querySelectorAll('[data-w-id="6e15c14e-1146-0b20-61d1-6721f0646d5a"]');
        webflowLinks.forEach(function(link) {
          // Supprimer l'attribut data-w-id pour dÃ©sactiver le comportement Webflow
          link.removeAttribute('data-w-id');
        });

        // Appliquer notre propre comportement Ã  tous les liens internes
        var allInternalLinks = document.querySelectorAll('a[href^="#"]:not([href="#"])');

        allInternalLinks.forEach(function(link) {
          if (!link.closest('.mobile-menu-list')) {
            link.addEventListener('click', function(e) {
              e.preventDefault();
              e.stopImmediatePropagation();

              var targetId = link.getAttribute('href');
              console.log('Scrolling to:', targetId); // Pour dÃ©boguer
              scrollToSectionTop(targetId);
            });
          }
        });

        // Script mobile inchangÃ©
        var menuLinks = document.querySelectorAll('.mobile-menu-list a');
        var menuToggle = document.getElementById('menu-toggle');

        menuLinks.forEach(function(link) {
          link.addEventListener('click', function(e) {
            e.preventDefault();
            var targetId = link.getAttribute('href');
            menuToggle.checked = false;

            setTimeout(function() {
              scrollToSectionTop(targetId);
            }, 300);
          });
        });

      }, 100); // Petit dÃ©lai pour s'assurer que tout est chargÃ©
    });
    </script>
